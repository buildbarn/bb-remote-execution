proto_library(
    name = "bazeloutputservice_proto",
    srcs = ["bazel_output_service.proto"],
    visibility = ["//visibility:public"],
    deps = [
        "@googleapis//google/rpc:status_proto",
        "@protobuf//:any_proto",
